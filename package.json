{
  "name": "win-sv",
  "version": "0.0.1",
  "description": "Wrapper for WinSW.exe, OS service manager, event logging, UAC, and several helper methods for interacting with Windows services.",
  "author": "Sorin Muresan <sorinm025@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/vision10/node-win-sv.git"
  },
  "keywords": [
    "windows",
    "service",
    "services",
    "daemon",
    "logging",
    "event",
    "event logging",
    "elevate",
    "process",
    "winsw",
    "manager",
    "install"
  ],
  "main": "index.js",
  "os": [
    "win32"
  ],
  "engines": {
    "node": ">=8"
  },
  "bin": {
    "service": "./bin/cli.js"
  },
  "scripts": {
    "test": "node ./examples/service.js",
    "uninstall": "node ./examples/service.js --uninstall"
  },
  "dependencies": {
    "xml": "^1.0.1",
    "yargs": "^17.3.1"
  }
}